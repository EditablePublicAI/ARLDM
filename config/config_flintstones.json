    },
    "images": {
      "data_field": "images",
      "generated": true
    },
    "captions": {
      "data_field": "captions",
      "generated": true
    },
    "attention_mask": {
      "data_field": "attention_mask",
      "generated": true
    },
    "source_caption": {
      "data_field": "source_caption",
      "generated": true
    },
    "source_attention_mask": {
      "data_field": "source_attention_mask",
      "generated": true
    }
  },
  "dataset": "flintstones",
  "new_tokens": ["fred", "barney", "wilma", "betty", "pebbles", "dino", "slate"],
  "max_length": 91,
  "clip_embedding_tokens": 49412,
  "blip_embedding_tokens": 30525,
  "ptms": {
    "clip_vit_base_patch16": "/anonymous/PTMs/clip-vit-base-patch16",
    "clip_vit_large_patch14": "/anonymous/PTMs/clip-vit-large-patch14",
    "stable_diffusion_v1_4": "/anonymous/PTMs/stable-diffusion-v1-4",
